{
  "name": "webnodelegacy",
  "version": "1.0.5",
  "description": "a high reliable python web library",
  "main": "__init__.py",
  "files": [
    "controller.py",
    "model.py",
    "options.json",
    "restController.py"
  ],
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS"
    ],
    "name": "webnode",
    "tagline": "a high reliable python web library<!--deDE:eine Pythonbibliothek für hochzuverlässigkeits Ansprüche im Web-->"
  },
  "scripts": {
    "clear": "rm apiDocumentation --recursive --force && find './' -type f -name '*.pyc' -delete && find './' -type f -name '*.pyo' -delete",
    "document": "node_modules/boostnode/runnable/generateAPIDocumentation.py",
    "update": "npm run clear && npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info"
  },
  "dependencies": {
    "boostnode": "^1.0.6"
  },
  "devDependencies": {
    "documentation-website": "^1.0.61"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/webnode"
  },
  "keywords": [
    "python",
    "web",
    "framework",
    "rest",
    "api",
    "backend"
  ],
  "homepage": "http://torben.website/webnode",
  "license": "CC-BY-3.0"
}
